<div id="<%= dom_id schedule %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Date:</strong>
    <%= schedule.date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Chart:</strong>
    <%= schedule.chart.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Court:</strong>
    <%= schedule.room.court.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Room:</strong>
    <%= schedule.room.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Case:</strong>
    <%= schedule.case.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Rapporteur:</strong>
    <%= schedule.rapporteur %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Judge:</strong>
    <%= schedule.judge %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Attorney:</strong>
    <%= schedule.attorney %>
  </div>
</div>
